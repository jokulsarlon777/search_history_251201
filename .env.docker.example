# Docker 배포용 환경 변수 템플릿
# VM의 실제 IP 주소로 변경하세요

# VM IP Address (⚠️ 반드시 변경 필요)
VM_IP=192.168.1.100

# Frontend URLs (브라우저에서 접근)
NEXT_PUBLIC_REACT_AGENT_URL=http://192.168.1.100:2025
NEXT_PUBLIC_LANGGRAPH_URL=http://192.168.1.100:2024
NEXT_PUBLIC_ELASTICSEARCH_URL=http://192.168.1.100:9200

# Assistant IDs
NEXT_PUBLIC_REACT_ASSISTANT_ID=react_agent
NEXT_PUBLIC_LANGGRAPH_ASSISTANT_ID=Deep Researcher

# API Keys (필요시 설정)
NEXT_PUBLIC_LANGGRAPH_API_KEY=

# Backend Internal URLs (컨테이너 간 통신)
INTERNAL_REACT_AGENT_URL=http://react-agent:2025
INTERNAL_LANGGRAPH_URL=http://deep-research-agent:2024
INTERNAL_ELASTICSEARCH_URL=http://elasticsearch:9200

# Elasticsearch 설정
ELASTICSEARCH_HEAP_SIZE=512m
ELASTICSEARCH_NETWORK_HOST=0.0.0.0

# 로깅 레벨
LOG_LEVEL=info
